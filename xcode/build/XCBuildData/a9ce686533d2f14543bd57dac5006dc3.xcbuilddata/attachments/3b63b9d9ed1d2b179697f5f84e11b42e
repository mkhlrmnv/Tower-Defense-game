#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/mkhlrmnv/Desktop/cpp_project/tower-defence/xcode
  /opt/homebrew/Cellar/cmake/3.27.7/bin/cpack -C $CONFIGURATION$EFFECTIVE_PLATFORM_NAME --config ./CPackConfig.cmake
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/mkhlrmnv/Desktop/cpp_project/tower-defence/xcode
  /opt/homebrew/Cellar/cmake/3.27.7/bin/cpack -C $CONFIGURATION$EFFECTIVE_PLATFORM_NAME --config ./CPackConfig.cmake
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/mkhlrmnv/Desktop/cpp_project/tower-defence/xcode
  /opt/homebrew/Cellar/cmake/3.27.7/bin/cpack -C $CONFIGURATION$EFFECTIVE_PLATFORM_NAME --config ./CPackConfig.cmake
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/mkhlrmnv/Desktop/cpp_project/tower-defence/xcode
  /opt/homebrew/Cellar/cmake/3.27.7/bin/cpack -C $CONFIGURATION$EFFECTIVE_PLATFORM_NAME --config ./CPackConfig.cmake
fi

